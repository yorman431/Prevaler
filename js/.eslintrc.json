{
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": [
    "eslint:recommended",
    "jquery"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ]
  }
}