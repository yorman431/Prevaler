{
  "env": {
    "browser": true,
    "jquery": true
  },
  "extends": [
    "eslint:recommended",
    "jquery"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "off",
      "windows"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "space-in-parens": [
      "error", 
      "never"]
  }
}